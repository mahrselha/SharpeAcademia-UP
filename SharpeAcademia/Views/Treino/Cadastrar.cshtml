@model Domain.Treino

@{ 
    var teste = ViewBag.Exercicios;
}
<form method="post">
    <fieldset>
        <legend>Treino</legend>

        @teste
            <div class="form-group" id="ExercicioNome">
                <select asp-for="NomeExercicio"
                     asp-items="@ViewBag.Exercicio"></select>
            </div>

        <div class="form-group" id="ProfessorNome">
            <select asp-for="Professor.Nome"
                    asp-items="@ViewBag.Professor"></select>
        </div>

        <div class="form-group" id="ClienteNome">
            <select asp-for="Cliente.Nome"
                    asp-items="@ViewBag.Cliente"></select>
        </div>


        <input type="submit"
               class="btn btn-success"
               value="Cadastrar"
               asp-action="Cadastrar"
               asp-controller="Treino" />
    </fieldset>
</form>